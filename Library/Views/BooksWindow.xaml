<Window x:Class="Library.Views.BooksWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Library.Views"
        mc:Ignorable="d"
        Title="BooksWindow"
        Height="600" Width="800" AllowsTransparency="True" Background="MidnightBlue"
         WindowStyle="None" WindowStartupLocation="CenterScreen"
        >
    <Grid>
        <Canvas>
            <Rectangle Fill="MidnightBlue" RadiusX="20" RadiusY="20" Width="700" Height="600">
                <Canvas.Left>50</Canvas.Left>
            </Rectangle>
        </Canvas>
    </Grid>
</Window>
